{
  "name": "MSPL",
  "version": "1",
  "manifest_version": 3,
  "description": "Open Power BI  Report in New Tab.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "tts", "scripting", "tabs", "storage"],
  
  
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "host_permissions": [
    "https://ice-fang.vercel.app/"
  ]
}

